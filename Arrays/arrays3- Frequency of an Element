#include<bits/stdc++.h>
using namespace std;
# define max_size 1024
void freq(int a[],int n);
void freq(int a[],int n)
{
	int h[max_size]={0},i,j;
    for(i=0;i<n;i++)
    {
    	h[a[i]]++;
	}
	
	for(j=0;j<n;j++)
	{
		if(h[a[j]]!=0)
		{
		  cout<<"frequency of"<< a[j]<<" : "<<h[a[j]]<<"\n";
		  h[a[j]]=0;
		}
   }
}
int main()
{
	int a[]={1,1,1,2,3,8,2,5};
	int size=sizeof(a)/sizeof(a[0]);
	freq(a,size);
	return 0;
}
